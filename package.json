{
  "name": "@notwoods/eslint-config-preact",
  "version": "1.0.0",
  "description": "Truly unopinionated base ESLint configuration for Preact projects.",
  "keywords": [
    "eslint",
    "eslint-config",
    "react",
    "preact"
  ],
  "repository": "NotWoods/eslint-config-preact",
  "license": "MIT",
  "author": "Tiger Oakes",
  "main": "dist/eslint-config-preact.js",
  "module": "dist/eslint-config-preact.module.js",
  "exports": {
    ".": {
      "import": "./dist/eslint-config-preact.module.js",
      "require": "./dist/eslint-config-preact.js"
    }
  },
  "files": [
    "configs"
  ],
  "scripts": {
    "format": "prettier --write .",
    "test": "eslint index.js && npm run -s build && vitest"
  },
  "dependencies": {
    "eslint-plugin-react": "^7.35.0"
  },
  "devDependencies": {
    "@types/eslint": "^8.56.11",
    "@types/node": "^20.0.0",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3"
  },
  "peerDependencies": {
    "eslint": ">=8.21.0 < 10"
  }
}
